					"time" : {
						"__type" : "dateAndTime",
						"dateAndTimeString" : "2023-06-12T09:08:09.923516+02:00"
					}
				},
				"editEmail" : {
					"__type" : "email",
					"emailString" : "<unknown>"
				},
				"editTime" : {
					"__type" : "time",
					"time" : {
						"__type" : "dateAndTime",
						"dateAndTimeString" : "2023-06-12T09:08:09.923516+02:00"
					}
				},
				"uid" : {
